local function stopNoclip()
	noclip = false
	if noclipConn then
		noclipConn:Disconnect()
		noclipConn = nil
	end

	if character then
		for _, v in ipairs(character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = true
			end
		end
	end
end

