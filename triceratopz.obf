local function spawnFloatingDinoTop(parentFrame)
	spawn(function()
		while parentFrame.Parent do
			local dino = Instance.new("TextLabel")
			dino.Text = "ðŸ¦–"
			dino.Font = Enum.Font.GothamBold
			dino.TextSize = math.random(20, 30)
			dino.TextColor3 = Color3.fromRGB(255, 255, 255)
			dino.BackgroundTransparency = 1
			dino.ZIndex = 99998

			-- Position alÃ©atoire en largeur sur la frame, en haut
			local xPos = math.random(0, parentFrame.AbsoluteSize.X - 20)
			dino.Position = UDim2.new(0, xPos, 0, -20) -- juste au-dessus de la frame
			dino.Parent = parentFrame

			-- Animation vers le haut
			local tweenService = game:GetService("TweenService")
			local goal = {Position = UDim2.new(0, xPos, 0, -70), TextTransparency = 1}
			local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Linear)
			local tween = tweenService:Create(dino, tweenInfo, goal)
			tween:Play()

			tween.Completed:Connect(function()
				dino:Destroy()
			end)

			task.wait(0.3) -- spawn un nouveau dino toutes les 0.3s
		end
	end)
end

