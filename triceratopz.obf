local function getClosestNPC()
	local closestDist = math.huge
	local closest = nil

	for _, npc in pairs(Players:GetPlayers()) do
		if npc ~= player and player.Character then
		if npc.Character:FindFirstChild("HumanoidRootPart") then
			local hrp = npc.Character.HumanoidRootPart
			local screenPos, onScreen = camera:WorldToViewportPoint(hrp.Position)
			if onScreen then
				local mousePos = UserInputService:GetMouseLocation()
				local dist = (Vector2.new(screenPos.X, screenPos.Y) - Vector2.new(mousePos.X, mousePos.Y)).Magnitude
				if dist < closestDist and dist <= aimbotFOV then
					closestDist = dist
					closest = hrp
				end
			end
		end
		end
	end

	return closest
end

local defaultMouseIcon
defaultMouseIcon = UserInputService.MouseIcon
local aimingMouseIcon = "http://www.roblox.com/asset/?id=5513462875"

local aimbotConn
local function startAimbot()
	if aimbotEnabled then return end
	aimbotEnabled = true

	aimbotConn = RunService.RenderStepped:Connect(function()
		if not aimbotEnabled then return end

		circle.Position = UDim2.fromScale(0.5, 0.5)

		local target = getClosestNPC()
		if target then
			UserInputService.MouseIconEnabled = true
			UserInputService.MouseIcon = aimingMouseIcon

			local hrpPos = target.Position
			local camPos = camera.CFrame.Position
			local direction = (hrpPos - camPos).Unit
			local currentLook = camera.CFrame.LookVector
			local newLook = currentLook:Lerp(direction, aimbotSensitivity)
			camera.CFrame = CFrame.new(camPos, camPos + newLook)
		else
			UserInputService.MouseIconEnabled = true
			UserInputService.MouseIcon = defaultMouseIcon
		end
	end)
end

local function stopAimbot()
	aimbotEnabled = false
	if aimbotConn then
		aimbotConn:Disconnect()
		aimbotConn = nil
	end
	circle:Destroy()
end

