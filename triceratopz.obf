local enabled = true

local function spamremotes(delayBetween)
	delayBetween = tonumber(delayBetween) or 0.01
	enabled = true

	local payloads = {
		{player},
		{player, character},
		{player, character, humanoid},
		{player, character, hrp},

		{1}, {0}, {-1},
		{5, 7, 9, 1, 4, 6, 2, 8, 47, 6},
		{5, 4, 5, 789, 4, 1, 1001, 6, 2, 6, 47, 6},

		{character},
		{character, humanoid},
		{character, humanoid, 40},
		{character, hrp, 5},

		{CFrame.new()},
		{CFrame.new(5, 8, 9)},
		{Vector3.new()},
		{CFrame.new(5, 8, 9), Vector3.new(0, 0, 0)},

		{CFrame.new(), character, player, 1, 2, 3},
		{CFrame.new(), 5, character, hrp, player, 1, 4, 6, 2, 8, 47, 6},

		{true},
		{false},
		{nil},
		{true, false},
		{nil, nil},

		{{}},
		{{a = 1}},
		{{player, character}},
	}

	for _, obj in ipairs(ReplicatedStorage:GetDescendants()) do
		if obj:IsA("RemoteEvent") then
			print("[SPAM] RemoteEvent :", obj:GetFullName())

			task.spawn(function()
				local index = 1

				while enabled do
					local args = payloads[index]

					pcall(function()
						obj:FireServer(unpack(args))
					end)

					index += 1
					if index > #payloads then
						index = 1
					end

					task.wait(delayBetween)
				end
			end)
		end
end

	wait(60)
	enabled = false
end

