btn.BorderColor3 = Color3.new(0,0,0)
btn.BorderSizePixel = 2
btn.Text = "execute"
btn.TextColor3 = Color3.new(1,1,1)
btn.TextScaled = true
btn.ZIndex = 999999

-- ===== COMMAND HANDLER =====
btn.MouseButton1Click:Connect(function()
	local cmd = string.lower(box.Text)

	if cmd == "fly" then
		startFly()

	elseif cmd == "unfly" then
		stopFly()

	elseif cmd == "fling" then
		startFling()
	elseif cmd == "unfling" then
		stopFling()
	elseif cmd == "noclip" then
		startNoclip()
	elseif cmd == "unnoclip" then
		stopNoclip()
	elseif string.sub(cmd,1,2) == "tp" then
		local targetName = string.sub(cmd,4)
		if targetName ~= "" then
			tpToPlayer(targetName)
		end
	elseif string.sub(cmd, 1, 5) == "speed" then
		local spd = string.match(cmd, "^speed%s+(%d+)$")
		if spd then
			speedchar(tonumber(spd))
		end
	elseif string.sub(cmd, 1, 8) == "playanim" then
		local animId = string.match(cmd, "^playanim%s+(%d+)$")
		if animId then
			playAnimationOnCharacter(animId)
		end
	elseif cmd == "esp" then
		startESP()
	elseif cmd == "noesp" then
		stopESP()
	elseif cmd == "discord" or cmd == "dc" then
		discord()
	elseif cmd == "cmds" or cmd == "cmd" or cmd == "commands" or cmd == "command" then
		commands()
	elseif cmd == "infjump" or cmd == "infinitejump" or cmd == "infinite jump" or cmd == "infinite jumps" or cmd == "inf jump" or cmd == "inf jumps" then
		infiniteJump()
	elseif cmd == "aimbot" then
		startAimbot()
	elseif cmd == "unaimbot" then
		stopAimbot()
	elseif cmd == "spamremotes" then
		spamremotes(0.01)
	end

end)

